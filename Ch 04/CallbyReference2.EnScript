class MainClass{  
   void swap(int & x, int & y) 
   {
      int temp = x;
      x = y;
      y = temp;
   }
   void Main(CaseClass c) 
   {
       int x = 5;
       int y = 7;
       Console.WriteLine("x = {0}, y = {1}", x, y);
       swap(x, y);
       Console.WriteLine("x = {0}, y = {1}", x, y);
   }
}