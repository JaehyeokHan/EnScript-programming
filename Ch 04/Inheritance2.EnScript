class Animal
{
  String  Name;
  uint age;
  double weight,
          height;
}
class Cat : Animal
{
  Cat() 
  {
    Name = “cuty”;
    age = 3;
    weight = 3.2;
    height = 50.2;
  }
}
class MainClass {
  void Main(CaseClass c) {
    Cat myCat();
    Console.WriteLine( “고양이 이름 : ” + myCat.Name );
    
    myCat.Name = “Nancy”;
    Console.WriteLine( “바뀐 고양이 이름 : ” + myCat.Name );
  
  }
}